<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" thread-count="3" parallel="tests">
<listeners>
	<listener class-name="com.auth0.qa.listeners.MyListeners"></listener>
</listeners>
  <test name="FirefoxTest">
  <parameter name="BrowserName" value="Firefox"></parameter>
    <classes>
      <class name="com.auth0.qa.testcases.SignupTest"/>
    </classes>
  </test>
  
  <test name="ChromeTest">
  <parameter name="BrowserName" value="Chrome"></parameter>
    <classes>
      <class name="com.auth0.qa.testcases.SignupTest"/>
    </classes>
  </test>
  
  <test name="SafariTest">
  <parameter name="BrowserName" value="Safari"></parameter>
    <classes>
      <class name="com.auth0.qa.testcases.SignupTest"/>
    </classes>
  </test>
</suite>